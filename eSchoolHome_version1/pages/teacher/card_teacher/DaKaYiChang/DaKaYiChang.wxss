/* pages/DaKaYiChang/DaKaYiChang.wxss */
.head{
  display: flex;
  width: 100%;
  height: 48px;
  line-height: 48px;
  background-color: rgba(187, 187, 187, 0.356);
}
.label1,.label2{
  width: 33.3%;
}
.label2{
  border-bottom: 3px solid rgb(32, 201, 111);
  background-color: rgb(200, 243, 220);
}
.label3{
  position: absolute;
  right: 10px;
}

.message{
  display: flex;
  width: 390px;
  height: 40px;
  line-height: 40px;
  background-color: rgba(255, 255, 255, 1);
  color: rgba(16, 16, 16, 1);
  font-size: 16px;
  font-family: Arial;
  border: 1px solid rgba(187, 187, 187, 1);
}
.id{
  width: 10px;
  margin-left: 15px;
}
.name{
  width: 50px;
  margin-left: 30px;
} 
.s_id{
  margin-left: 40px;
} 
.abnormal{
  width: 110px;
  position: absolute;
  right: 0;
  color: rgb(255, 0, 0);
}