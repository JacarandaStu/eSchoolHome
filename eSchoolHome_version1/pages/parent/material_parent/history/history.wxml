<view class="page-text">
  <text class="head">历史任务:</text>
</view>
<view class="task-list">
    <block wx:for="{{record}}" wx:for-item="item" wx:key="key">
      <view class="area_css" bindtap='go' id="{{item.id}}">
        <view><text class='begin-time'>{{item.timestamp}}</text></view>
        <view><text class='task-title'>{{item.title}}</text></view>
        <view><text class='end-time'>{{item.ddl}}</text></view>
      </view>
    </block>
</view>